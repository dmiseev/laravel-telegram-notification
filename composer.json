{
    "name": "dmiseev/laravel-telegram-notification",
    "version": "1.0",
    "description": "Telegram Notification for Laravel framework",
    "keywords": [
        "laravel",
        "telegram",
        "notification",
        "telegram notification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Aseev",
            "email": "dmiseev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "*",
        "illuminate/support": "*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "3.4.x-dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dmiseev\\TelegramNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmiseev\\TelegramNotification\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmiseev\\TelegramNotification\\TelegramNotificationProvider"
            ]
        }
    }
}
